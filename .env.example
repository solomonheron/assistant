# Database
DATABASE_URL=postgresql://user:password@localhost:5432/ai_assistant

# Redis for Celery
REDIS_URL=redis://localhost:6379/0

# AI Model API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Model Configuration (optional - defaults are set in config.py)
SMALL_MODEL=gpt-4o-mini
MEDIUM_MODEL=gpt-4o
LARGE_MODEL=claude-3-5-sonnet-20241022

# App Settings
DEBUG=True
APP_NAME=AI Assistant

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:5000,http://0.0.0.0:5000
